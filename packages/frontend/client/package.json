{
  "name": "@dapperduckling/oauth-monitor-client",
  "version": "1.0.7",
  "description": "An opinionated library securing frontend applications without all the fuss",
  "keywords": [
    "keycloak",
    "sso",
    "express",
    "fastify",
    "authentication",
    "oidc"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "DapperDuckling Dev LLC",
  "license": "AGPL-3.0-only",
  "type": "module",
  "bugs": {
    "url": "https://github.com/DapperDuckling/oauth-monitor/issues"
  },
  "homepage": "https://github.com/DapperDuckling/oauth-monitor#readme",
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "npm run clean && tsc",
    "yalc:publish": "yalc publish --push",
    "prepublishOnly": "npm run build",
    "prepare": "ts-patch install && typia patch"
  },
  "devDependencies": {
    "ts-patch": "^3.3.0",
    "typescript": "^5.4.2"
  },
  "files": [
    "dist",
    "src",
    "COPYING",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@dapperduckling/oauth-monitor-common": "file:../../common",
    "typia": "^5.2.6"
  },
  "yalcWatch": {
    "watchFolder": "dist",
    "buildWatchCommand": "tsc --watch",
    "extensions": "js,ts"
  },
  "gitHead": "a686cd38071ba0da7bdceb7c03008e570e9de0c1"
}
