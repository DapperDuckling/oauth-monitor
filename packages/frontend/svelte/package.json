{
  "name": "@dapperduckling/oauth-monitor-svelte",
  "version": "1.0.0",
  "description": "An opinionated library for securing Svelte apps without all the fuss",
  "keywords": [
    "svelte",
    "keycloak",
    "sso",
    "authentication",
    "oidc"
  ],
  "author": "DapperDuckling Dev LLC",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/DapperDuckling/oauth-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/DapperDuckling/oauth-monitor/issues"
  },
  "homepage": "https://github.com/DapperDuckling/oauth-monitor#readme",
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "npm run clean && svelte-package",
    "yalc:publish": "yalc publish --push",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "COPYING",
    "README.md"
  ],
  "peerDependencies": {
    "svelte": ">=4.0.0"
  },
  "dependencies": {
    "@dapperduckling/oauth-monitor-client": "file:../client",
    "@dapperduckling/oauth-monitor-common": "file:../../common"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "~5.2.0",
    "vite": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
