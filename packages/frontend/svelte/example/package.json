{
  "name": "svelte-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "yalc:add": "npm run yalc:add:svelte && npm run yalc:add:client && npm run yalc:add:common && npm i",
    "yalc:add:svelte": "yalc add --dev @dapperduckling/oauth-monitor-svelte",
    "yalc:add:client": "yalc add --dev @dapperduckling/oauth-monitor-client",
    "yalc:add:common": "yalc add --dev @dapperduckling/oauth-monitor-common"
  },
  "devDependencies": {
    "@dapperduckling/oauth-monitor-client": "file:.yalc/@dapperduckling/oauth-monitor-client",
    "@dapperduckling/oauth-monitor-common": "file:.yalc/@dapperduckling/oauth-monitor-common",
    "@dapperduckling/oauth-monitor-svelte": "file:.yalc/@dapperduckling/oauth-monitor-svelte",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "autoprefixer": "^10.4.19"
  },
  "type": "module"
}
