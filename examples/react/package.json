{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:parcel\" \"npm run start:server\"",
    "start:parcel": "npm run clean && parcel -p 3000 src/index.html",
    "start:server": "node server.js",
    "clean": "rimraf ./.parcel-cache/",
    "yalc:add": "npm run yalc:add:react && npm run yalc:add:client && npm run yalc:add:common && npm i",
    "yalc:add:react": "yalc add --dev @dapperduckling/oauth-monitor-react",
    "yalc:add:client": "yalc add --dev @dapperduckling/oauth-monitor-client",
    "yalc:add:common": "yalc add --dev @dapperduckling/oauth-monitor-common"
  },
  "devDependencies": {
    "@dapperduckling/oauth-monitor-client": "file:.yalc/@dapperduckling/oauth-monitor-client",
    "@dapperduckling/oauth-monitor-common": "file:.yalc/@dapperduckling/oauth-monitor-common",
    "@dapperduckling/oauth-monitor-react": "file:.yalc/@dapperduckling/oauth-monitor-react",
    "@types/react": "^18.0.13",
    "@types/react-dom": "^18.0.5",
    "@parcel/watcher": "~2.1.0",
    "parcel": "^2.6.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5"
  },
  "dependencies": {}
}
